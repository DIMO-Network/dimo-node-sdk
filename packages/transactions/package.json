    "ethers": "^6.13.3",
    "lodash": "^4.17.21",
    "permissionless": "^0.1.45",
    "viem": "^2.21.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
